{% extends "templates/default/base.njk" %}

{% block main %}
<div class="light small timetable">
  {% include "templates/elements/notifications.njk" %}
  <table id="stoptimes">
    <tbody>
      {% for stop in data.stops %}
      {% include "templates/elements/stop-item.njk" %}
      <tr>
        <td data-context="default.normal">&nbsp;</td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
  {% block content %}
  {% endblock content %}
</div>
{% endblock main %}
